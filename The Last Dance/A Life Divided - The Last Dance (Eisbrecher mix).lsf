    1000ms: setrgb A 0ms > 255,160,0    #orange
    2000ms: setrgb A 0ms > 255,255,0    #yellow
    3000ms: setrgb A 0ms > 0,255,0      #green
    4000ms: setrgb A 0ms > 0,255,100    #teal
    5000ms: setrgb A 0ms > 0,200,200    #azure
    6000ms: setrgb A 0ms > 0,0,255      #blue
    7000ms: setrgb A 0ms > 190,0,255    #violet
    8000ms: setrgb A 0ms > 255,0,170    #pink
    9000ms: setrgb A 0ms > 255,255,255  #white
    10000ms: end
16000ms: end

@IntroBeat_B2
0ms: loop 16000ms
