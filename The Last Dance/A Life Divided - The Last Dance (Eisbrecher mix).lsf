    6500ms: setrgb A 0ms > 255,255,0    #yellow
    7500ms: setrgb A 0ms > 0,255,0      #green
    8500ms: setrgb A 0ms > 0,0,255      #blue
    9500ms: setrgb A 0ms > 0,255,100    #teal
    10000ms: end
16000ms: end


